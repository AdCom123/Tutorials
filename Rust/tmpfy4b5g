struct Rechteck {
    x: f64,
    y: f64,
}
trait FlaecheDa {
    fn flaeche(&self) -> f64;
}

impl FlaecheDa for Rechteck {
	fn new() -> Rechteck {
		Rechteck{x: 0.0, y: 2.0,}
	}
    fn flaeche(&self) -> f64 {
        self.x * self.y
    }
    fn umfang(&self) -> f64 {
    	2.0 * (self.x + self.y)
    }
    fn destroy(self) -> Rechteck {
    	self
    }
}

fn print_fla(arg: Type) -> RetType {
    unimplemented!();
}

fn main() {
	let r = Rechteck::new().umfang();
	println!("{}", r);
}