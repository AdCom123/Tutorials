struct Rechteck {
    x: f64,
    y: f64,
}

impl Rechteck {
	fn new() -> Rechteck {
		Rechteck{x: 0.0, y: 0.0,}
	}
    fn flaeche(&self) -> f64 {
        self.x * self.y
    }
    fn umfang(&mut self) -> f64 {
    	self.x = 3.0;
    	2.0 * (self.x + self.y)
    }
    fn destroy(self) -> Rechteck {
    	self
    }
}

fn main() {
	let r = Rechteck::new().flaeche();
	println!("{}", r);
}