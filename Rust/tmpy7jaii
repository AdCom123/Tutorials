struct Rechteck {
    x: f64,
    y: f64,
}

impl Rechteck {
    fn flaeche(&self) -> f64 {
        self.x * self.y
    }

    fn umfang(&self) -> f64 {
    	2.0 * (self.x + self.y)
    }
    fn destroy(self) {
    	let Rechteck(x,b) = self;
    }
}

fn main() {
	let mut r = Rechteck{x: 65.0, y: 2.0};
	r.destroy();
	println!("{}", r.umfang());
}